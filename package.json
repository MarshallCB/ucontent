  },
  "module": "esm/index.js",
  "dependencies": {
    "csso": "^4.0.3",
    "html-escaper": "^3.0.0",
    "html-minifier": "^4.0.0",
    "terser": "^4.7.0",
    "uhyphen": "^0.1.0",
    "umap": "^1.0.2",
    "uparser": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/ucontent.git"
