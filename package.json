{
  "name": "ucontent",
  "version": "1.6.0",
  "description": "An SSR oriented HTML content generator",
  "main": "cjs/index.js",
  "scripts": {
    "build": "npm run cjs && npm run test && npm run bench",
    "bench": "node test/benchmark.js",
    "cjs": "ascjs esm cjs",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc node test/index.js"
